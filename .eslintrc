{
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "no-undef": 2,
      "no-unused-vars": 1,
      "react/display-name": 0,
      "react/prop-types": 0,
      "no-console": 0,
      "no-extra-semi": 0,
      "react/no-unescaped-entities": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}